{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Koala",
	"scopeName": "source.koala",
	"patterns": [
	  {
		"include": "#comments"
	  },
	  {
		"include": "#keywords"
	  },
	  {
		"include": "#functions"
	  },
	  {
		"include": "#strings"
	  },
	  {
		"include": "#operators"
	  },
	  {
		"include": "#variables"
	  },
	  {
		"include": "#errors"
	  }
	],
	"repository": {
	  "comments": {
		"name": "comment.line.double-slash.koala",
		"match": "//.*"
	  },
	  "keywords": {
		"patterns": [
		  {
			"name": "keyword.control.koala",
			"match": "\\b(if|while|for|return|reason_mat_do|throw)\\b"
		  },
		  {
			"name": "keyword.other.koala",
			"match": "\\bthoda_idhar_aye\\b"
		  }
		]
	  },
	  "functions": {
		"patterns": [
		  {
			"name": "entity.name.function.koala",
			"match": "\\b(log|ek_kaam_kro)\\b"
		  }
		]
	  },
	  "strings": {
		"patterns": [
		  {
			"name": "string.quoted.double.koala",
			"begin": "\"",
			"end": "\"",
			"patterns": [
			  {
				"name": "constant.character.escape.koala",
				"match": "\\\\."
			  }
			]
		  }
		]
	  },
	  "operators": {
		"name": "keyword.operator.koala",
		"match": "(\\+|\\-|\\*|\\/|=)"
	  },
	  "variables": {
		"name": "variable.other.koala",
		"match": "\\b\\w+\\b"
	  },
	  "errors": {
		"patterns": [
		  {
			"name": "support.type.exception.koala",
			"match": "\\b(zerodivisionerror|ioerror|attributeerror|datatypeerror|importerror|typeerror)\\b"
		  }
		]
	  }
	},
	"fileTypes": ["koala"],
	"uuid": "01234567-89ab-cdef-0123-456789abcdef",
	"publisher": "KrushnaVyas"
  }
  